{
  "name": "linter-write-good",
  "main": "./lib/init",
  "version": "0.9.0",
  "private": true,
  "description": "Naive linter for English prose for developers who can't write good and wanna learn to do other stuff good too.",
  "repository": {
    "type": "git",
    "url": "https://github.com/AtomLinter/linter-write-good.git"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=1.9.0 <2.0.0"
  },
  "dependencies": {
    "write-good": "0.13.1",
    "xregexp": "2.0.0"
  },
  "providedServices": {
    "linter": {
      "versions": {
        "2.0.0": "provideLinter"
      }
    }
  },
  "renovate": {
    "extends": [
      "config:base"
    ],
    "semanticCommits": true,
    "rangeStrategy": "pin",
    "packageRules": [
      {
        "packagePatterns": [
          "^eslint"
        ],
        "groupName": "ESLint packages"
      }
    ]
  }
}
